{
    "core.log": {
		"scope": "mcfunction",
		"prefix": "log",
		"body": [
			"data merge storage smithed:log {message:'$1',level:1,type:'${2:INFO}'}",
			"function #smithed:core/pub/technical/tools/log"
		],
		"description": "Send message to Smithed Log"
	},
	"core.damage": {
		"scope": "mcfunction",
		"prefix": "damage",
		"body": [
			"scoreboard players set @s smithed.damage ${1:1}",
			"function #smithed:core/pub/entity/damage/apply"
		],
		"description": "Damage an entity by n"
	}
}